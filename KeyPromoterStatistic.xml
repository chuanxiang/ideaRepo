<application>
  <component name="KeyPromoterStatistic">
    <option name="statistics">
      <Statistic Action="Add">
        <Item>
          <ShortCut>'⌘N' or '⌃⏎'</ShortCut>
          <Description>Add</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Compare with Clipboard">
        <Item>
          <IdeaActionID>CompareClipboardWithSelection</IdeaActionID>
          <ShortCut>'⌘+B'</ShortCut>
          <Description>Compare with Clipboard</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Copy Reference">
        <Item>
          <IdeaActionID>CopyReference</IdeaActionID>
          <ShortCut>'⌥⇧⌘C'</ShortCut>
          <Description>Copy Reference</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Find Action...">
        <Item>
          <IdeaActionID>GotoAction</IdeaActionID>
          <ShortCut>'⇧⌘A'</ShortCut>
          <Description>Find Action...</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Generate...">
        <Item>
          <IdeaActionID>Generate</IdeaActionID>
          <ShortCut>'⌘N' or '⌃⏎'</ShortCut>
          <Description>Generate...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Hide">
        <Item>
          <ShortCut>'⇧⎋'</ShortCut>
          <Description>Hide</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Jump to Source">
        <Item>
          <IdeaActionID>EditSource</IdeaActionID>
          <ShortCut>'⌘↓' or 'F4'</ShortCut>
          <Description>Jump to Source</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project">
        <Item>
          <IdeaActionID>ActivateProjectToolWindow</IdeaActionID>
          <ShortCut>'⌘+1'</ShortCut>
          <Description>Project</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project Structure...">
        <Item>
          <IdeaActionID>ShowProjectStructureSettings</IdeaActionID>
          <ShortCut>'⌘;'</ShortCut>
          <Description>Project Structure...</Description>
          <Count>5</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'TestClass.main()'">
        <Item>
          <IdeaActionID />
          <ShortCut>'⌃⇧R'</ShortCut>
          <Description>Run 'TestClass.main()'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Synchronize">
        <Item>
          <IdeaActionID>Synchronize</IdeaActionID>
          <ShortCut>'⌥⌘Y'</ShortCut>
          <Description>Synchronize</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>